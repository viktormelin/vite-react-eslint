{
  "arrowParens": "always",
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "endOfLine": "auto",
  "quoteProps": "consistent",
  "proseWrap": "never",
  "trailingComma": "es5",
  "importOrder": [
    "^react",
    "^\\w",
    "/types/",
    "/hooks/",
    "/store/",
    "/views/",
    "/components/",
    "/assets/",
    "^./",
    "css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
